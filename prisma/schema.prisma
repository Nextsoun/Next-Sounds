generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Contact {
  id        String   @id @default(uuid()) @db.Uuid
  name      String
  email     String
  phone     String
  message   String
  createdAt DateTime @default(now())
}
